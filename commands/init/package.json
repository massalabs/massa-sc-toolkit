{
    "name": "my-massa-sc",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "postinstall": "node ./simulator/simulate.js --install",
        "test": "npx astester --imports node_modules/@massalabs/massa-as-sdk/astester.imports.js",
        "asbuild:debug": "asc assembly/index.ts --target debug",
        "asbuild:release": "asc assembly/index.ts --target release",
        "asbuild": "npm run asbuild:debug && npm run asbuild:release",
        "build": "asc assembly/index.ts --target release --exportRuntime -o build/index.wasm && asc --transform @massalabs/as-transformer/file2base64.js assembly/main.ts --target release --exportRuntime -o build/main.wasm",
        "simulate": "node ./simulator/simulate.js",
        "deploy": "ts-node --esm deployer/deployment_script.ts",
        "lint": "eslint --resolve-plugins-relative-to \"./assembly/**/*.{ts,json}\" --fix",
        "prettier": "as-prettier \"./*.{ts,js,json,md}\" \"./{src,__tests}/**/*.{ts,js,json,md}\" --write",
        "fmt": "npm run lint && npm run prettier"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@massalabs/as": "^1.0.2",
        "@massalabs/as-tester": "^0.0.2-dev",
        "@massalabs/as-transformer": "^0.0.2-dev",
        "@massalabs/massa-as-sdk": "^1.1.1-dev",
        "@massalabs/massa-web3": "^1.10.0",
        "@types/node": "^18.11.3",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "assemblyscript": "^0.21.7",
        "assemblyscript-prettier": "^1.0.5",
        "dotenv": "^16.0.3",
        "eslint": "^8.25.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "tslib": "^2.4.0"
    },
    "overrides": {
        "assemblyscript": "^0.21.7"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./build/release.js",
            "types": "./build/release.d.ts"
        }
    }
}
