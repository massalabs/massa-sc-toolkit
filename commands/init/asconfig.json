{
    "entries": ["assembly/impl.ts"],
    "targets": {
        "debug": {
            "outFile": "build/impl.wasm",
            "sourceMap": true,
            "debug": true
        },
        "release": {
            "outFile": "build/impl.wasm",
            "sourceMap": true,
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": false
        }
    },
    "options": {
        "exportRuntime": true,
        "bindings": "esm",
        "transform": ["@massalabs/as-transformer/file2ByteArray.js", "@massalabs/as-tester/check_replacer.js"]
    }
}
